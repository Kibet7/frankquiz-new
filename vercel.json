{
  "builds": [
    { "src": "myproject/asgi.py", "use": "@vercel/python" },
    { "src": "manage.py", "use": "@vercel/static-build", "config": { "distDir": "staticfiles" } }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "myproject/asgi.py" }
  ]
}
